{
  "scripts": {
    "prestart": "npx webpack --mode=production",
    "start": "node app.js",
    "dev:start": "nodemon app.js --watch \"./dist\"",
    "dev:webp": "webpack --mode=development --watch",
    "dev": "npm-run-all --parallel dev:start dev:webp"
  },
  "type": "module",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "express": "^4.17.3",
    "webgl-basic-lib": "file:../WebGL-basic-lib",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "css-loader": "^6.7.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "source-map-loader": "^1.0.0",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.9.2"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ],
    "delay": 250
  }
}
